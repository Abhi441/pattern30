# pattern30



import java.util.*;
class Main{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
       //row
    int row = 1;
    while(row<= n){
        int val = 5;
    
      // work space
    int col = 1;
    while(col<= n){
      System.out.print(val);
      val--;
      col++;
    }
      
     
    
       System.out.println();
       row = row +1;
       
    }
    
  }
}
